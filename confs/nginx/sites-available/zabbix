server {
  listen 80;
  listen [::]:80;
  
  root /var/www/dominio.com;

  index index.php index.html index.htm;
  server_name dominio.com;

  location / {
    try_files $uri $uri/ =404;
  }

  location ~ .php$ {
    include snippets/fastcgi-php.conf;
    fastcgi_pass unix:/var/run/php/phpPHP_V-fpm.sock;
  }

}
